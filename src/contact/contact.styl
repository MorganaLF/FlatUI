.contact
  max-width: 900px
  padding: 80px 0
  &__container
    //width: 50%
    display: flex
    flex-wrap: wrap
    margin: 0 auto
  &__col
    &:first-child
      width: 70%
      padding-right: 5%
    &:last-child
      width: 30%
  &__subtitle
    margin: 10px 0 20px
    font-size: 22px
  &__list
    margin-bottom: 40px
    list-style-type: none
    font-size: 18px
    &-item
      margin-bottom: 5px
  &__list-span
    display: inline-block
    margin-right: 10px
    font-weight: 900
  .map__container
    margin-bottom: 40px




